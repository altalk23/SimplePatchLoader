{
    "geode": "1.3.0",
    "version": "v1.0.0",
    "id": "cgytrus.patcher",
    "name": "Simple Patch Loader",
    "developer": "ConfiG",
    "description": "meow :3",
    "early-load": true,
    "resources": {
        "files": [
            "resources/shaders/*.glsl"
        ]
    },
    "settings": {
        "blur-fast": {
            "name": "Blur Fast",
            "type": "bool",
            "default": false
        },
        "blur-amount": {
            "name": "Blur Amount",
            "type": "float",
            "default": 0.1,
            "min": 0.0,
            "max": 1.0,
            "control": {
                "slider": true
            }
        },
        "blur-time": {
            "name": "Blur Time",
            "type": "float",
            "default": 0.1,
            "min": 0.0,
            "max": 5.0,
            "control": {
                "slider": true
            }
        }
    },
    "dependencies": [
        {
            "id": "hjfod.custom-keybinds",
            "version": "v1.0.0",
            "importance": "required"
        }
    ]
}
